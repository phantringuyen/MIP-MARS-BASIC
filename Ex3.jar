# Nhap vao mot ky tu hoa, in ra ký tu thuong.
.data  
	Nhap: .asciiz "Nhap vao 1 ki tu IN HOA : "
  	KiTu: .asciiz"\nKi tu "
 	La: .asciiz " chuyen sang thuong la: "
.text  

main:  
# xuat ham "Nhap" va nhap ki tu
  la $a0, Nhap  
  jal InRa
  addi $v0, $0, 12  
  syscall  
  
  addi $s0, $v0, 0  

# xuat "Ki tu"
  la $a0, KiTu  
  jal InRa 

# xuat ra ki tu da nhap
  addi $a0, $s0, 0  
  addi $v0, $0, 11  
  syscall  
  
# xuat "la"
  la $a0, La  
  jal InRa 
  
# ki tu thuong can tim
  addi $a0, $s0, 32  
  addi $v0, $0, 11  
  syscall  

exit:  
  addi $v0, $0, 10  
  syscall   

InRa:  
  addi $v0, $0, 4  
  syscall  
  jr $ra
